{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "/home/site/wwwroot/bin/linux/deno",
      "arguments": [
        "run",
        "--allow-net",
        "--allow-env",
        "--allow-read",
        "worker.ts"
      ]
    }
  }
}